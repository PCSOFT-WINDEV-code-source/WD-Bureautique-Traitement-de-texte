     name : InitialiseRuban
     internal_properties : CAAAAAgAAAC5sNoRWiJqVuk+KJsqkXicTqOgnpAbZumqrY7DKaoiBQ0qDniFrP8igZe8/5VxmaCjyg2lf2e8wkPg+K9Dq8gjA+U2Iln5dq1RIqzTARm8yFrklOC/tOUs9fo690+G9Mfwns29u472eV6GQOgMUlttq96XUNxpkY0IaM5RPYLrnDUvvN8MCIKGBf/u0NUQuSL9141D/PJalwqsXj6+YzoLRm3qkSMyTdNEgNfTQ4GGU0nwPTncvGRlmltV38dTJEkqYty7ACVi
     procedure_id : 1879870016887287398
     type_code : 14
     starred : true
     code : |1-
      // Résumé : Initialise le ruban personnalisé (obligatoire après le code d'initialisation de la fenêtre)
      // Syntaxe :
      //InitialiseRuban (<sRubanSource> est chaîne [, <oRubanPersonnalisé> est champ [, <pMasquageRubanCallback> est procédure]])
      //
      // Paramètres :
      //	sRubanSource (chaîne ANSI) : Nom du ruban natif
      //	oRubanPersonnalisé (champ - valeur par défaut=0) : Champ ruban personnalisé
      // 	pMasquageRubanCallback (procédure - valeur par défaut=0) : Callback personnalisée de masquage du ruban
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE InitialiseRuban(sRubanSource est une chaine, oRubanPersonnalisé est un champ = null, pMasquageRubanCallback est une procédure = null)
      
      	// Rend la fenêtre visible
      	gFenDocument..visible = vrai
      
      	// Vérifie l'existence du champ du ruban source
      	SI ChampExiste(sRubanSource) = faux ALORS
      		
      		// Il n'existe pas : il a été personnalisé ou il s'agit de la barre d'outils standard, on ignore
      		retour
      	FIN
      		
      	oRubanNatif est un champ <- {sRubanSource, indChamp}
      	
      	// Applique le même style que la fenêtre
      	oRubanNatif..Image 	= gsImageVoletRuban
      	oRubanNatif..Style 	= RUBAN_STYLE..Style
      	oRubanNatif..Image 	= gsImageVoletRuban
      		
      	// Masque le premier regroupement du ruban (ses fonctionnalités correspondent au menu fichier géré par le modèle)
      	sNomRegroupement est une chaine = EnumèreChamp(oRubanNatif[1]..nomcomplet, 1)
      	si sNomRegroupement <> "" alors {oRubanNatif.nomcomplet+"."+sNomRegroupement, indchamp}..visible = faux
      	
      	// Si on a fournit une callback personnalisée pour le masquage du ruban
      	si pMasquageRubanCallback <> null alors
      		// Appelle la callback personnalisée de masquage du ruban
      		pMasquageRubanCallback( oRubanNatif )
      	fin
      	
      	// Change la couleur de fond
      	oCouleur est une couleur = LIB_FOND_BARRE_TITRE..CouleurFond
      	oRubanNatif..CouleurFond = oCouleur
      	
      	// Génère l'image de fond 5 états pour les boutons
      	avec oCouleur
      	
      		oImageBoutonStyle est une image
      		oImageBoutonStyle..Largeur = 100
      		oImageBoutonStyle..hauteur = 20
      		
      		// Début du dessin
      		dDébutDessin(oImageBoutonStyle, dAvecOpacité)
      		
      		// Enfoncé
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 180
      		..Luminosité = 95
      		..Opacité = 50
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(Transparent)
      		drectangle(20, 0, 39, 19)
      
      		// Actif
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 180
      		..Luminosité = 115
      		..Opacité = 50
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(Transparent)
      		drectangle(60, 0, 79, 19)
      		
      		// Survol
      		oCouleur = LIB_FOND_BARRE_TITRE..CouleurFond
      		..Saturation = 30
      		..Luminosité = 160
      		..Opacité = 40
      		
      		dfond(..Couleur, FondPlein, 0, ..Opacité)
      		dStylo(transparent)
      		drectangle(80, 0, 99, 19)		
      		
      		// Sauvegarde
      		gsImageBoutonStyle = fRepDonnéesUtilisateur() + [fsep] + "btn5.png"
      		dSauveImagePNG(oImageBoutonStyle, gsImageBoutonStyle)
      	fin
      	
      	// Applique l'image générée sur les boutons déclarés
      	pour tout oChamp de gtabChampsStyle
      		si oChamp = null alors continuer
      			
      		// Applique le style
      		__AppliqueStyle(oChamp)
      	FIN
      			
      	// Insère les volets personnalisés dans le ruban natif : permet de conserver les mises à jour du ruban "natif"
      	si oRubanPersonnalisé <> null alors
      		RubanDupliqueVolets(oRubanPersonnalisé, oRubanNatif)	
      	fin
      		
      	// Déplace les boutons d'UNDO/REDO
      	DéplaceUndoRedo()
      	
      	// Déplace le libellé de lecture seule
      	DéplaceLectureSeule()
      	
      fin:	
      	// Le ruban est mis à jour toutes les 250ms
      	TimerSys(SurMiseAJourRuban, 250ms)
     type : 458752
   -
     name : SurMiseAJourRuban
     procedure_id : 1879870021182320495
     type_code : 14
     group : 5
     code : |1-
      // Résumé : Mise à jour du ruban <À SURCHARGER SI NÉCESSAIRE>
      // Syntaxe :
      // SurMiseAJourRuban ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      procedure SurMiseAJourRuban()
      			
      	// Si les champs natifs existent
      	SI ChampExiste("BTN_WDFAA_Redo") = Vrai _ET_ ChampExiste("BTN_WDFAA_Undo") = Vrai ALORS
      				
      		EXTERNE BTN_WDFAA_Redo, BTN_WDFAA_Undo
      				
      		// Copie l'état des boutons s'ils ont changé
      		si BTN_RACC_REDO..Etat <> BTN_WDFAA_Redo..Etat alors BTN_RACC_REDO..etat = BTN_WDFAA_Redo..Etat
      		si BTN_RACC_UNDO..Etat <> BTN_WDFAA_Undo..Etat alors BTN_RACC_UNDO..etat = BTN_WDFAA_Undo..Etat			
      	fin
     type : 458752
   -
     name : MenuListeDocumentsRécents
     procedure_id : 1879917549236275680
     type_code : 14
     code : |1-
      // Résumé : Liste les documents récemment ouverts
      // Syntaxe :
      //MenuListeDocumentsRécents ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MenuListeDocumentsRécents()
      	
      	sablier( vrai )
      	
      	// Désactive l'affichage
      	ZR_DOCUMENTS_RECENTS..AffichageActif = faux
      	BTN_OUVRIR_PARCOURIR..Visible = faux
      	
      	// Signale le début du remplissage
      	ZR_DOCUMENTS_RECENTS.DébutRemplissage()
      
      	// Vide la liste
      	ZR_DOCUMENTS_RECENTS.SupprimeTout()
      
      	// La MRU est déjà triée
      	pour tout stElement de COL_PARAMETRAGE.MRU
      		
      		// Si l'extension du fichier ne correspond pas à celle paramétrée, on l'ignore
      		si position(COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR, "*." + fExtraitChemin(stElement.CheminComplet, fExtension)[[2 a]], 1, SansCasse + MotComplet) < 1 ALORS continuer
      	
      		// Si le document n'existe plus, on l'ignore
      		SI fFichierExiste(stElement.CheminComplet) = Faux ALORS CONTINUER
      	
      		dhDate est une dateheure = DateHeureUTCVersLocale( stElement.DateHeureAccès )
      	
      		// Charge les informations fichier du document				
      		nLigne est un entier = ZR_DOCUMENTS_RECENTS.Ajouteligne( stElement.CheminComplet, DateVersChaîne(dhDate..PartieDate, maskDateDuréeRelative), *, stElement.PositionDoc)
      		
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_DATE = dhDate
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_DOSSIER = DécomplèteRep(fExtraitChemin(stElement.CheminComplet, fDisque + fRépertoire))
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_NOM_FICHIER = fExtraitChemin(stElement.CheminComplet, fFichier + fExtension)
      	FIN
      		
      	// Signale la fin du remplissage
      	ZR_DOCUMENTS_RECENTS.finRemplissage()
      	ZR_DOCUMENTS_RECENTS.SelectMoins()
      	
      	// Réactive l'affichage
      	BTN_OUVRIR_PARCOURIR..visible = vrai
      	ZR_DOCUMENTS_RECENTS..AffichageActif = vrai
      	
      	Sablier( faux )
