     name : MenuNouveauDocument
     internal_properties : CAAAAAgAAACeGZo5bZyFSbIi7+3CfqN6nywcRPIbnQYduDDVBUDGDoQTcVTBsgDtEHjqlb64ec6ikl5jXqjNg7pm9WbUpsx5KdI4dNiQLxbAyMLH7yS7qDmW9qiovYKhb0QfJwLXT24ehZZmj1D8p+uOvFs69ncaA2FlzLbhxe4LgM9b2EeF+FT+uHzlNHvWgi/BN4ryiyh2/dOqZW4=
     procedure_id : 1879968088100952080
     type_code : 14
     code : |1-
      // Résumé : Crée un document
      // Syntaxe :
      //MenuNouveauDocument ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      PROCEDURE MenuNouveauDocument()
      
      	sAlias est une chaine = "DOC_" + dateheuresys		
      	
      	// Ferme le menu fichier
      	MenuFichierFerme()
      		
      	// Ouvre un alias de la fenêtre
      	OuvreSoeur(sAlias + " = " + gFenDocument..Nom + SansFocus)
      	
      	// Maximise la fenêtre si on est maximisé
      	si fentaille(gFenDocument) = taillemaxi alors FenTaille(sAlias, taillemaxi)
      
      	// Prend une référence sur les actions de l'alias
      	pstDocumentActions est une action dynamique = {sAlias + ".DocumentAction", indvariable}
      
      	// Appelle la callback de création
      	si pstDocumentActions.CréeDocument = null _ou_ pstDocumentActions.CréeDocument() = vrai ALORS
      		
      		// Charge les informations du fichier
      		ExécuteTraitement(sAlias + ".MenuAfficheInfos", trtProcédure, ( "" ))
      	
      		// Donne le focus à la fenêtre ouverte
      		SysDonneFocus( Handle(sAlias) )
      		
      		// Incrémente le compteur
      		COL_PARAMETRAGE.gnCompteurDocument ++
      	FIN
      	
      CAS EXCEPTION:
      	dbgAssertion( faux, exceptioninfo )
      	retour
