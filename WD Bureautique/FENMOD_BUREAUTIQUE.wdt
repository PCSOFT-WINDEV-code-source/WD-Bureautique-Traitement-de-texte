   -
     name : MenuRécupèreDernièrePosition
     procedure_id : 1905567063857599672
     type_code : 14
     code : |1+
      // Résumé : Récupère la position d'un fichier dans la MRU
      // Syntaxe :
      //[ <Résultat> = ] MenuRécupèreDernièrePosition (<sFichier> est chaîne)
      //
      // Paramètres :
      //	sFichier (chaîne ANSI) : Chemin complet du fichier
      // Valeur de retour :
      // 	entier : Position mémorisée dans la MRU pour le fichier demandé
      //
      PROCEDURE MenuRécupèreDernièrePosition(LOCAL sFichier est une chaine) : entier
      
      // Récupère l'indice du fichier dans la MRU
      nIndice est un entier = COL_PARAMETRAGE.MRU.cherche(tclineaire, "CheminComplet", sFichier)
      // Si le fichier n'est pas trouvé, renvoie une position par défaut
      si nIndice <= 0 alors renvoyer 0
      
      // Renvoie la position mémorisée
      renvoyer COL_PARAMETRAGE.MRU[nIndice].PositionDoc
     type : 458752
