   -
     name : DéplaceUndoRedo
     procedure_id : 1884748601528774370
     type_code : 14
     code : |1-
      // Résumé : Déplace les boutons d'UNDO/REDO
      // Syntaxe :
      // DéplaceUndoRedo ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE DéplaceUndoRedo()
      
      	// Si les boutons natifs de raccourcis UNDO/REDO existent
      	SI ChampExiste("BTN_WDFAA_Redo") = Vrai _ET_ ChampExiste("BTN_WDFAA_Undo") = Vrai ALORS
      		
      		EXTERNE BTN_WDFAA_Redo, BTN_WDFAA_Undo
      		
      		// Masque les boutons UNDO et REDO du ruban natif		
      		BTN_WDFAA_Redo..Visible = Faux
      		BTN_WDFAA_Undo..Visible = Faux
      		
      		// Affiche les raccourcis d'UNDO/REDO
      		BTN_RACC_UNDO..Visible = Vrai
      		BTN_RACC_REDO..Visible = Vrai
      	SINON
      		
      		// Masque les raccourcis d'UNDO/REDO
      		BTN_RACC_UNDO..Visible = Faux
      		BTN_RACC_REDO..Visible = Faux
      	FIN
     type : 458752
