     name : MenuFermeDocument
     internal_properties : CAAAAAgAAACdXtf2n4sN74JZ13M5+j6lKQS7u/SPuT7UxPqB8tlIB5yTDbbiX7wRvLTTzszQ3hxtxtvKbZKFPt2MtBm9GziP4ZarUTWHBh9w3+LjYZiasit4eNopi/FEHV9jC7eM8Z8KwU4LnJQQsCWcN5PCaniy1qpWpLUElkv5YOnkJ18FayQOC+8aIUlNJ03NjLje99ZktEkP20QHa3an20nGtzfeJKZARW62FFL/C+OkgDaeA6NzbFRrqN0V7FaGHxgjhR2Ws7T+Kg==
     procedure_id : 1881398187667034874
     type_code : 14
     code : |1-
      // Résumé : Ferme le document en cours
      // Syntaxe :
      //[ <Résultat> = ] MenuFermeDocument ( [<bFermeFenêtre> est booléen])
      //
      // Paramètres :
      //	bFermeFenêtre (booléen - valeur par défaut=1) : Vrai pour forcer la fermeture de la fenêtre, Faux sinon
      // Valeur de retour :
      // 	booléen : Vrai si la fermeture a été effectuée avec succès, Faux dans le cas contraire
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE MenuFermeDocument(local bFermeFenêtre est un booléen = vrai) : booléen
      	
      	// Si le document a été modifié
      	SI gbDocumentModifié ALORS
      		
      		//1 : Enregistrer
      		//2 : Ne pas enregistrer
      		//3 : Annuler
      		selon Dialogue(<§?128C491E00466BE7§>) 
      			
      			// Enregistrer
      			cas 1 
      			
      				// Sauvegarde le document
      				si MenuSauveDocument() = -1 ALORS
      					RENVOYER Faux
      				FIN
      		
      			// Ne pas enregistrer
      			cas 2
      				
      				// On ne fait rien
      			
      			// Annuler
      			cas 3
      				// On arrête tout et on reprend
      				renvoyer faux
      		FIN
      	FIN
      
      	// Déclenche la fermeture
      	SI DocumentAction.FermeDocument = Null _OU_ DocumentAction.FermeDocument( gsCheminDocument ) = Vrai ALORS
      			
      		// Mémorise le document dans la MRU
      		MRUAjoute( gsCheminDocument, gxDocPosition )
      	
      		// Ferme
      		si bFermeFenêtre alors ferme()	
      		RENVOYER vrai
      	fin
      	
      RENVOYER Faux
      	
      CAS EXCEPTION:
      	dbgAssertion( Faux, ExceptionInfo )
      	SI bFermeFenêtre ALORS ferme()
      	RENVOYER Faux
