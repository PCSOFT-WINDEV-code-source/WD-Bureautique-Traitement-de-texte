     name : MenuListeDocumentsRécents
     internal_properties : CAAAAAgAAACGGhBKasTs3BkJhWOtHhgnaeRBuo2L8Qc79IGlTSs9ieQTNew024+q7QjRGEKn9i/x6ddG2HAN0Tl949xDQr0FYP3+/lcNye7o525fJkINogyIr6wb4BWbRvgfXRuW7iTKqWU=
     procedure_id : 1879964523289880669
     type_code : 14
     code : |1-
      // Résumé : Liste les documents récemment ouverts
      // Syntaxe :
      //MenuListeDocumentsRécents ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MenuListeDocumentsRécents()
      	
      	sablier( vrai )
      	
      	// Désactive l'affichage
      	ZR_DOCUMENTS_RECENTS..AffichageActif = faux
      	BTN_OUVRIR_PARCOURIR..Visible = faux
      	
      	// Signale le début du remplissage
      	ZR_DOCUMENTS_RECENTS.DébutRemplissage()
      
      	// Vide la liste
      	ZR_DOCUMENTS_RECENTS.SupprimeTout()
      
      	// La MRU est déjà triée
      	pour tout stElement de COL_PARAMETRAGE.MRU
      		
      		// Si l'extension du fichier ne correspond pas à celle paramétrée, on l'ignore
      		si position(COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR, "*." + fExtraitChemin(stElement.CheminComplet, fExtension)[[2 a]], 1, SansCasse + MotComplet) < 1 ALORS continuer
      	
      		// Si le document n'existe plus, on l'ignore
      		SI fFichierExiste(stElement.CheminComplet) = Faux ALORS CONTINUER
      	
      		dhDate est une dateheure = DateHeureUTCVersLocale( stElement.DateHeureAccès )
      	
      		// Charge les informations fichier du document				
      		nLigne est un entier = ZR_DOCUMENTS_RECENTS.Ajouteligne( stElement.CheminComplet, DateVersChaîne(dhDate..PartieDate, maskDateDuréeRelative), *, stElement.PositionDoc)
      		
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_DATE = dhDate
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_DOSSIER = DécomplèteRep(fExtraitChemin(stElement.CheminComplet, fDisque + fRépertoire))
      		ZR_DOCUMENTS_RECENTS[nLigne].LIB_NOM_FICHIER = fExtraitChemin(stElement.CheminComplet, fFichier + fExtension)
      	FIN
      		
      	// Signale la fin du remplissage
      	ZR_DOCUMENTS_RECENTS.finRemplissage()
      	ZR_DOCUMENTS_RECENTS.SelectMoins()
      	
      	// Réactive l'affichage
      	BTN_OUVRIR_PARCOURIR..visible = vrai
      	ZR_DOCUMENTS_RECENTS..AffichageActif = vrai
      	
      	Sablier( faux )
     type : 458752
   -
     name : _TT_MiseEnMaigre
     procedure_id : 1879969179040494410
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE _TT_MiseEnMaigre()
      
      // Récupère le fragment en cours
      FragmentStyle est un DocFragment = __FragmentStyle()
      
      // RAZ du style
      FragmentStyle.MiseEnForme.PoliceItalique = faux
      FragmentStyle.MiseEnForme.PoliceGras = faux
      FragmentStyle.MiseEnForme.PoliceBarrée = faux
      FragmentStyle.MiseEnForme.CouleurSurlignage = Transparent
      FragmentStyle.MiseEnForme.CouleurTexte = Noir
      FragmentStyle.MiseEnForme.AlignementVertical = alignementBaseline
      
      // Redonne le focus
      DonneFocus(TT_Principal)
      
      // Résumé : retourne le fragment a utiliser pour appliquer un style
      	Procédure interne __FragmentStyle()
      	FragmentCourant est docFragment <- TT_Principal..Sélection..Fragment
      	
      	// si aucune sélection et que le curseur est dans un mot, on utilise le mot
      	SI TT_Principal..LongueurSélection=0
      		SI FragmentCourant..Mot..Occurrence=1 ALORS
      			RENVOYER FragmentCourant..Mot[1]
      		FIN
      	FIN
      	RENVOYER FragmentCourant
      	fin
     type : 458752
   -
     name : _TT_ChangeMajuscule
     procedure_id : 1879969230580179092
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE _TT_ChangeMajuscule()
      
      // Récupère le fragment en cours
      FragmentMajuscule est un DocFragment = __ExtraitMot()
      
      // Variable temporaire
      sTexte est une chaîne = FragmentMajuscule.Texte
      sPremièreLettre est une chaine = sTexte[1]
      
      selon Vrai
      	// Tout minuscule => 1 lettre majuscule
      	cas FragmentMajuscule.texte = Minuscule(sTexte) 
      		FragmentMajuscule.Texte = Majuscule(sPremièreLettre)  + (sTexte[2 À])
      		
      	// 1 lettre majuscule => tout majuscule
      	cas FragmentMajuscule.texte = Majuscule(sPremièreLettre) + Minuscule(sTexte[2 À])
      		FragmentMajuscule.Texte = Majuscule(sTexte)
      		
      	// Tout majuscule => tout minuscule + les autres cas 
      	AUTRE CAS
      		// cas tout majuscule ou tous les autres cas
      		FragmentMajuscule.Texte = Minuscule(sTexte)
      FIN
      
      
      FIN:
      // Redonne le focus
      DonneFocus(TT_Principal)
      
      
      	// Résumé : retourne le fragment a utiliser pour appliquer un style
      	PROCÉDURE INTERNE __ExtraitMot()
      	FragmentCourant est docFragment <- TT_Principal..Sélection..Fragment
      	
      	// si aucune sélection et que le curseur est dans un mot, on utilise le mot
      	SI TT_Principal..LongueurSélection=0
      		SI FragmentCourant..Mot..Occurrence=1 ALORS
      			RENVOYER FragmentCourant..Mot[1]
      		FIN
      	FIN
      	RENVOYER FragmentCourant
      	FIN
