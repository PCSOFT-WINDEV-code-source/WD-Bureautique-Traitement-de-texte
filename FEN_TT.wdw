     name : DéclencheAssociationAutomatique
     internal_properties : 
     procedure_id : 1882824692329547164
     type_code : 14
     code : |1+
      // Résumé : Déclenche la demande d'association automatique
      // Syntaxe :
      //DéclencheAssociationAutomatique ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Procédure automatique :
      // La procédure est exécutée automatiquement, après le code d'initialisation de la fenêtre, avec un différé de 2 secondes
      // 
      PROCEDURE DéclencheAssociationAutomatique()
      
      	// Opération d'association non disponible en mode test
      	SI EnModeTest() = Vrai ALORS retour
      		
      	// Si on a déjà enregistré l'association
      	SI ChargeParamètre("FileExtAssociation", "") = "Done" ALORS retour
      
      	// Si on a déjà demandé l'association
      	SI ChargeParamètre("FileExtAssociation\Prompted", "") = "Yes" ALORS retour
      	
      	// On marque qu'on pose la question
      	SauveParamètre("FileExtAssociation\Prompted", "Yes")
      	
      	// xxxxx (*.yyy)
      	sTypeDocuments est une chaine = Minuscule( COL_PARAMETRAGE.DESCRIPTION_DOCUMENT[1] ) + COL_PARAMETRAGE.DESCRIPTION_DOCUMENT[2 a] + " ([%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%])"
      	
      	// On pose la question
      	si dialogue("Souhaitez-vous associer les [%sTypeDocuments%] à [%ProjetInfo(piNomProjet)%] ?", ["Associer", "Plus tard"], 1, 2) = 2 alors retour
      	
      	// Déclenche l'association		
      	AssocieExtensions( vrai )
      	
      	// Marque que l'association a été effectuée
      	SauveParamètre("FileExtAssociation", "Done")
      		
      	// Affiche un toast
      	ToastAffiche("Les fichiers [%COL_PARAMETRAGE.EXTENSION_DOCUMENT_SELECTEUR%] s'ouvriront désormais avec [%ProjetInfo(piNomProjet)%]", toastCourt, cvMilieu)
      		
      	// Mise à jour du bouton
      	BTN_ASSOCIATION.ExécuteTraitement( trtInit )
