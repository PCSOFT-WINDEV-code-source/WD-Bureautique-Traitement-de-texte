     code : |1-
      
      // Code spécifique à la création
      DocumentAction.CréeDocument = __CréeNouveauDocumentTexte
      
      // Code spécifique à l'ouverture
      DocumentAction.OuvreDocument = __OuvreTT
      
      // Code spécifique à la sauvegarde
      DocumentAction.SauveDocument = __SauveTT
      
      // SHIFT + F3 : Mise en majuscule 
      DéclareRaccourci(trShift + vk_f3, _TT_ChangeMajuscule)
      
      // CTRL + Espace : Enlève le style appliqué
      DéclareRaccourci(trControl + VK_SPACE, _TT_MiseEnMaigre)
      
      
      	PROCÉDURE INTERNE __CréeNouveauDocumentTexte() : booléen
      	
      	// RAZ du champ Traitement de texte
      	TT_Principal = ""
      	
      	RENVOYER Vrai
      	FIN
      
      	PROCÉDURE INTERNE __OuvreTT( sChemin est une chaîne ,  xDocPos est un numérique = 0) : booléen
      	
      	// Charge le fichier dans le champ Traitement de texte
      	si TT_Principal.Ouvre(sChemin) = Faux
      		TT_Principal = ""
      		Erreur(ErreurInfo((errRésumé)))
      		renvoyer faux
      	FIN
      	
      	// Position du champ TT
      	TT_Principal.Positionne(xDocPos,xDocPos,30,40)
      	TT_Principal..Curseur = xDocPos
      	TT_Principal..FinCurseur = xDocPos
      	
      	renvoyer vrai
      	
      	FIN
      
      	PROCÉDURE INTERNE __SauveTT( sChemin est une chaîne ) : booléen
      	
      	// Sauve le fichier
      	RENVOYER TT_Principal.Sauve(sChemin)
      	FIN
      
      FIN:
      
      // Initialise le ruban
      InitialiseRuban("RUBAN_WINDEVFAA_TT", Null, MasqueRegroupementDocument)
      
      // Crée un nouveau document
      DocumentAction.CréeDocument()
      
      // Premier plan
      MaFenêtre..Plan = PLAN_TT
