     internal_properties : CAAAAAgAAACGGhBKasTs3BkJhWOtHhgnaeRBuo2L8Qc79IGlTSs9ieQTNew024+q7QjRGEKn9i/x6ddG2HAN0Tl949xDQr0FYP3+/lcNye7o525fJkINogyIr6wb4BWbRvgfXRuW7iTKqWU=
     procedure_id : 1879969333663415751
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //MenuAfficheInfos (<sChemin> est chaîne)
      //
      // Paramètres :
      //	sChemin (chaîne ANSI) : <indiquez ici le rôle de sChemin>
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE MenuAfficheInfos(local sChemin est une chaine)
      
      	bLectureSeule est un booléen = ( BTN_MENU_NOUVEAU..Visible = Faux _et_ BTN_MENU_ENREGISTRER..visible = faux )
      
      	// En création
      	si sChemin = "" _ou_ fFichierExiste(sChemin) = faux ALORS
      		
      		LIB_INFO_TAILLE = "Pas encore enregistré"
      		LIB_INFO_DATEMODIFICATION = ""
      		LIB_INFO_DATECREATION = DateVersChaîne(datedujour, maskDateDuréeRelative) + [", "] + heureverschaine(maintenant, "HH:MM")
      		
      		BTN_OUVRIR_EMPLACEMENT..visible = faux
      		LIB_TITRE_INFORMATIONS_DISQUE..Visible = Faux
      		
      		// Nom et chemin du document
      		si bLectureSeule alors
      			
      			// Pas de nom par défaut en lecture seule
      			gsNomDocumentOuvert = ""
      		sinon
      			gsNomDocumentOuvert = "Document" + COL_PARAMETRAGE.gnCompteurDocument
      		FIN
      		gsCheminDocument = ""
      		
      	sinon
      		
      		// Affiche la taille formatée
      		LIB_INFO_TAILLE = TailleVersChaîne( ftaille(sChemin) )
      		
      		// Lit la date/heure de modification et de création
      		dhModification est une dateheure = fdateheure(sChemin, "", fModification)
      		dhCréation est une dateheure = fdateheure(sChemin, "", fCréation)
      		
      		// Formate l'affichage des dates
      		LIB_INFO_DATEMODIFICATION = DateVersChaîne(dhModification..partiedate, maskDateDuréeRelative) + [", "] + HeureVersChaîne(dhModification..partieheure, "HH:MM")
      		LIB_INFO_DATECREATION = DateVersChaîne(dhCréation..partiedate, maskDateDuréeRelative) + [", "] + HeureVersChaîne(dhCréation..partieheure, "HH:MM")
      		
      		BTN_OUVRIR_EMPLACEMENT..Visible = Faux
      		LIB_TITRE_INFORMATIONS_DISQUE..Visible = Faux
      			
      		// Nom et chemin du document
      		gsNomDocumentOuvert = fExtraitChemin(sChemin, fFichier + fExtension)	
      		gsCheminDocument = sChemin		
      		
      	FIN
      	
      	// Mise à jour du titre de la fenêtre
      	mafenetre..titre = ( gsNomDocumentOuvert + (gbDocumentModifié ? "*" sinon "") + (gbModeLectureSeule ? " ** LECTURE SEULE **" SINON "") ) + [" - "] + sansespace(extraitchaine(projetinfo(piNomProjet), 2, "WD Bureautique "))
