     procedure_id : 1882445451038797841
     type_code : 14
     code : |1-
      // Résumé : Callback personnalisée de masquage du ruban
      // Paramètres :
      //	oRuban (champ) : Ruban du champ TT
      // Valeur de retour :
      // 	Aucune
      //
      // 
      PROCEDURE MasqueRegroupementDocument(oRuban est un champ)
      
      	// On masque les 3 premiers regroupements du ruban (Nouveau / Ouvrir, Enregistrer, Imprimer / Exporter)
      	
      	mafenetre..AffichageActif = faux
      	
      	// Nouveau / Ouvrir
      	sRegroupement est une chaine = EnumèreChamp( oRuban, 1)
      	si sRegroupement <> "" alors {sRegroupement, indchamp}..visible = faux
      	
      	// Enregistrer
      	sRegroupement = EnumèreChamp( oRuban, 2)
      	si sRegroupement <> "" alors {sRegroupement, indchamp}..visible = faux
      	
      	// Imprimer / Exporter
      	sRegroupement = EnumèreChamp( oRuban, 3)
      	si sRegroupement <> "" alors {sRegroupement, indchamp}..visible = faux
      	
      	MaFenêtre..AffichageActif = vrai
