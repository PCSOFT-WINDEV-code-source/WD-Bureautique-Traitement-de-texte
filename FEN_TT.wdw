     name : MenuSauveDocument
     internal_properties : CAAAAAgAAACdXtf2n4sN74JZ13M5+j6lKQS7u/SPuT7UxPqB8tlIB5yTDbbiX7wRvLTTzszQ3hxtxtvKbZKFPt2MtBm9GziP4ZarUTWHBh9w3+LjYZiasit4eNopi/FEHV9jC7eM8Z8KwU4LnJQQsCWcN5PCaniy1qpWpLUElkv5YOnkJ18FayQOC+8aIUlNJ03NjLje99ZktEkP20QHa3an20nGtzfeJKZARW62FFL/C+OkgDaeA6NzbFRrqN0V7FaGHxgjhR2Ws7T+Kg==
     procedure_id : 1881398187666772730
     type_code : 14
     code : |1-
      // Résumé : Déclenche la sauvegarde du document
      // Syntaxe :
      //[ <Résultat> = ] MenuSauveDocument ( [<sCheminFichier> est chaîne [, <bForceSélectionParcours> est booléen]])
      //
      // Paramètres :
      //	sCheminFichier (chaîne UNICODE) : Chemin dans lequel sera sauvé le document (par défaut, chemin courant du document ouvert)
      //	bForceSélectionParcours (booléen - valeur par défaut=0) : Vrai si la sélection du fichier de sauvegarde doit être forcée
      // Valeur de retour :
      // 	entier : 0 ou 1 selon si le document a été sauvé, -1 si le dialogue [Enregistrer sous] a été déclenché
      //
      // Traitement automatique des exceptions :   exécuter le bloc de code CAS EXCEPTION:
      //
      
      PROCEDURE MenuSauveDocument(local sCheminFichier est une chaine = gsCheminDocument, local bForceSélectionParcours est un booléen = faux) : entier
      
      	// Le document n'a pas encore été sauvé ou s'il a été modifié en lecture seule
      	si sCheminFichier = "" _ou_ (gsCheminDocument = sCheminFichier _et_ gbModeLectureSeule) ALORS
      		
      		si bForceSélectionParcours = Faux ALORS
      			
      			// Si le plan affiché n'est pas un plan de menu
      			si mafenetre..plan < PLAN_MENU ALORS
      				
      				// Force l'ouverture du menu
      				BTN_MENU_FICHIER.ExécuteTraitement(trtclic)
      			FIN
      			
      			// Déclenche le traitement 'Enregistrer sous'
      			BTN_MENU_ENREGISTRER_SOUS.ExécuteTraitement(trtclic)		
      			renvoyer -1
      		sinon
      			
      			// Ouvre le sélecteur de fichier
      			sCheminFichier = OuvreSélecteurFichier()		
      			SI sCheminFichier = "" ALORS
      				renvoyer faux
      			FIN
      		FIN		
      	FIN
      	
      	// Déclenche la sauvegarde
      	si DocumentAction.SauveDocument = null _ou_ DocumentAction.SauveDocument( sCheminFichier ) = vrai ALORS
      		
      		// Fin des modifications et fin de la lecture seule		
      		gbDocumentModifié = faux
      		gbModeLectureSeule = faux
      		
      		// Mémorise le chemin
      		gsCheminDocument = sCheminFichier
      		
      		// Mise à jour
      		MenuAfficheInfos( gsCheminDocument )
      		
      		// Mémorise le document dans la MRU
      		MRUAjoute( sCheminFichier, gxDocPosition )
      			
      		// Affiche un toast
      		ToastAffiche("Document enregistré", toastCourt, cvMilieu)
      				
      		// Ferme le menu fichier
      		MenuFichierFerme()
      		
      		renvoyer vrai
      	FIN
      	
      renvoyer faux
      
      CAS EXCEPTION:
      	dbgAssertion( Faux, ExceptionInfo )
      	renvoyer faux
     type : 458752
